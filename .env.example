# Copy this file to `.env.local` and fill in real values.

# API credentials for Kvikmyndir
EXPO_PUBLIC_KVIKMYNDIR_USERNAME=your_username_here
EXPO_PUBLIC_KVIKMYNDIR_PASSWORD=your_password_here

